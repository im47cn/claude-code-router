# 数据库配置
DATABASE_URL="mysql://username:password@localhost:3306/claude_router_db"

# 飞书OAuth配置
FEISHU_APP_ID="your_feishu_app_id"
FEISHU_APP_SECRET="your_feishu_app_secret"
FEISHU_REDIRECT_URI="http://localhost:3000/auth/feishu/callback"

# JWT配置
JWT_SECRET="your_jwt_secret_key_here"
JWT_EXPIRES_IN="7d"

# Session配置
SESSION_SECRET="your_session_secret_key_here"

# API配置
API_BASE_URL="http://localhost:3000"
PORT=3000

# 配额默认配置
DEFAULT_USER_QUOTA_LIMIT=1000
DEFAULT_USER_QUOTA_WINDOW=86400  # 24小时
DEFAULT_API_KEY_QUOTA_LIMIT=500
DEFAULT_API_KEY_QUOTA_WINDOW=86400  # 24小时

# 安全配置
BCRYPT_ROUNDS=12
COOKIE_SECURE=false

# 日志配置
LOG_LEVEL="info"
LOG_REQUESTS=true
