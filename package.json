{
  "name": "@musistudio/claude-code-router",
  "version": "1.0.70",
  "description": "Use Claude Code without an Anthropics account and route it to another LLM provider",
  "bin": {
    "ccr": "dist/cli.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "release": "npm run build && npm publish",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run tests/unit",
    "test:integration": "vitest --run tests/integration",
    "test:e2e": "cd tests/20251028-01-auth && npx playwright test",
    "pretest": "./scripts/reset-test-db.sh",
    "posttest": "./scripts/stop-test-db.sh",
    "test:e2e:debug": "cd tests/20251028-01-auth && npx playwright test --debug",
    "test:e2e:report": "cd tests/20251028-01-auth && npx playwright show-report",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "validate": "node -e \"const { validateConfig, formatValidationResult } = require('./src/utils/configValidator'); const config = require('./config.json'); console.log(formatValidationResult(validateConfig(config)));\"",
    "perf:report": "node -e \"console.log(require('./src/utils/performanceMonitor').performanceMonitor.generateReport())\""
  },
  "keywords": [
    "claude",
    "code",
    "router",
    "llm",
    "anthropic"
  ],
  "author": "musistudio",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/session": "^10.9.0",
    "@fastify/static": "^8.2.0",
    "@musistudio/llms": "^1.0.44",
    "@inquirer/prompts": "^5.0.0",
    "dotenv": "^16.4.7",
    "find-process": "^2.0.0",
    "@prisma/client": "^6.2.0",
    "async-mutex": "^0.5.0",
    "bcrypt": "^5.1.1",
    "json5": "^2.2.3",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "minimist": "^1.2.8",
    "mysql2": "^3.14.0",
    "openurl": "^1.1.1",
    "rotating-file-stream": "^3.2.7",
    "shell-quote": "^1.8.3",
    "tiktoken": "^1.0.21",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.0.15",
    "@vitest/coverage-v8": "^4.0.4",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.1",
    "fastify": "^5.4.0",
    "playwright": "^1.56.1",
    "prisma": "^6.2.0",
    "shadcn": "^3.5.0",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.0",
    "typescript": "^5.8.2",
    "vitest": "^4.0.4"
  },
  "publishConfig": {
    "ignore": [
      "!build/",
      "src/",
      "screenshots/"
    ]
  }
}
